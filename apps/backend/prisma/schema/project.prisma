model Project {
  id      Int       @id @default(autoincrement())
  name    String    @db.VarChar(255)
  status  String    @db.VarChar(30) @default("Not Started")
  created DateTime  @default(now())
  due     DateTime?  
  ownerId Int?
  owner   User?     @relation(fields: [ownerId], references: [id])
  taskId  Int?
  tasks   Task[]
}
